			</xs:sequence>
			<xs:attribute name="deploymentMode" use="optional" default="production">
				<xs:simpleType>
					<xs:restriction base="xs:NMTOKEN">
						<xs:enumeration value="production"/>
						<xs:enumeration value="test"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:attribute>
			<xs:attribute name="inReplyTo" type="xs:string" use="optional"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="MessageType">
		<xs:complexType mixed="true"/>
	</xs:element>
	<xs:element name="Money">
		<xs:complexType mixed="true">
