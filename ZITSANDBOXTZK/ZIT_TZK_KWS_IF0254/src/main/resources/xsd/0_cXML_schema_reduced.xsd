				<xs:element ref="Tax" minOccurs="0"/>
				<xs:element ref="Distribution" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Contact" minOccurs="0" maxOccurs="unbounded"/>
				<xs:element ref="Comments" minOccurs="0"/>
