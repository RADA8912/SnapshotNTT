<?xml version="1.0" encoding="UTF-8"?>
<xs:schema version="1.0" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="supplier">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="account-number" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Account number</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="allow-change-requests" type="xs:integer" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Allows suppliers to create change requests from CSP</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="allow-csp-access-without-two-factor" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Allows supplier to access Customer's data from CSP without Two Factor</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="allow-cxml-invoicing" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Allow cXML invoicing for Supplier</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="allow-inv-choose-billing-account" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>allow_inv_choose_billing_account</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="allow-inv-from-connect" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>If yes, then the supplier can create invoices against their POs or Contracts in CSP</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="allow-inv-no-backing-doc-from-connect" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>If yes, then the supplier can create invoices without a backing PO or Contract in CSP.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="allow-inv-unbacked-lines-from-connect" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>If yes, then the supplier can create unbacked invoices without a backing PO or Contract in CSP.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="business-entity-id" type="xs:integer" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Business Entity</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="buyer-hold" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Hold all POs for buyer review</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="commodity" type="Commodity" minOccurs="0"/>
                <xs:element name="contacts" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="contact" minOccurs="0" maxOccurs="unbounded" type="Contact"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="content-groups" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="content-group" minOccurs="0" maxOccurs="unbounded" type="BusinessGroup"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="corporate-url" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>corporate_url</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="coupa-pay-financing-only" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Only pay financed invoices via Coupa Pay</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="created-by" type="UserSimple" minOccurs="0"/>
                <xs:element name="customer-support-contacts" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="customer-support-contact" minOccurs="0" maxOccurs="unbounded" type="CustomerSupportContact"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="cxml-domain" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>"From" , our domain</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-http-password" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Password required to access the Supplier's online store</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="cxml-http-username" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>User name required to access the Supplier's online store</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-identity" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>"From", our identity</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-invoice-buyer-domain" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Buyer Domain for cXML Invoicing</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-invoice-buyer-identity" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Buyer Identity for cXML Invoicing</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-invoice-secret" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Secret Key for cXML Invoicing Authentication</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-invoice-supplier-domain" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier Domain for cXML Invoicing</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-invoice-supplier-identity" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier Identity for cXML Invoicing</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-protocol" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Transmission protocol</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-secret" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Shared secret</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-ssl-version" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Specify the SSL version used for cXML communication with the supplier</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-supplier-domain" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>"To", supplier domain</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-supplier-identity" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>"To", supplier identity</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="cxml-url" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>URL where POs are sent if PO transmission is "cxml"</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="dd-settings" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Dynamic Discounting Settings whitelisted for supplier</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="default-locale" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Default Locale for sending emails to this supplier</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="disable-cert-verify" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Specify whether to ignore SSL certificate mismatch errors</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="display-name" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>display_name</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="diversities" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="diversity" minOccurs="0" maxOccurs="unbounded" type="DiversityAssociation"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="duns" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier DUNS number</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="enterprise" type="Enterprise" minOccurs="0"/>
                <xs:element name="hold-invoices-for-ap-review" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Prevent invoices from this supplier from being approved before AP reviews them.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="inventory-organization" type="InventoryOrganization" minOccurs="0"/>
                <xs:element name="invoice-emails" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="invoice-email" minOccurs="0" maxOccurs="unbounded" type="InvoiceEmail"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="invoice-matching-level" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Invoice matching level</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="2-way"/>
                            <xs:enumeration value="3-way"/>
                            <xs:enumeration value="3-way-direct"/>
                            <xs:enumeration value="none"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="name" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier name</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="100"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="number" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier number</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="on-hold" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier On Hold</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="online-store" type="OnlineStore" minOccurs="0"/>
                <xs:element name="parent" type="Supplier" minOccurs="0"/>
                <xs:element name="payment-method" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Default payment method, selectable from drop down</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="payment-term" type="PaymentTerm" minOccurs="0"/>
                <xs:element name="payment-term-id" type="xs:integer" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Payment Term ID</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="payment-terms" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="payment-term" minOccurs="0" maxOccurs="unbounded" type="PaymentTerm"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="po-change-method" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Purchase order change transmission method</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="cxml"/>
                            <xs:enumeration value="xml"/>
                            <xs:enumeration value="email"/>
                            <xs:enumeration value="prompt"/>
                            <xs:enumeration value="mark_as_sent"/>
                            <xs:enumeration value="buy_online"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="po-email" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Email where POs are sent if PO transmission is "email"</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="po-method" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Purchase order transmission method</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="cxml"/>
                            <xs:enumeration value="xml"/>
                            <xs:enumeration value="email"/>
                            <xs:enumeration value="prompt"/>
                            <xs:enumeration value="mark_as_sent"/>
                            <xs:enumeration value="buy_online"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="preferred-commodities" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="preferred-commodity" minOccurs="0" maxOccurs="unbounded" type="Commodity"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="primary-address" type="SupplierAddress" minOccurs="0"/>
                <xs:element name="primary-contact" type="Contact" minOccurs="0"/>
                <xs:element name="remit-to-addresses" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="remit-to-address" minOccurs="0" maxOccurs="unbounded" type="RemitToAddress"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="restricted-account-types" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="restricted-account-type" minOccurs="0" maxOccurs="unbounded" type="AccountType"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="savings-pct" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Savings for using this supplier</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:decimal">
                            <xs:totalDigits value="8"/>
                            <xs:fractionDigits value="2"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="scf-configs" type="xs:string" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supply Chain Finance Configurations whitelisted for supplier</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="send-invoices-to-approvals" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>If yes, then invoices will all be sent thru approvals, regardless of total amount.</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="shipping-term" type="ShippingTerm" minOccurs="0"/>
                <xs:element name="status" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier status</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="20"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="storefront-url" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier website</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="supplier-addresses" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="supplier-address" minOccurs="0" maxOccurs="unbounded" type="SupplierAddress"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="supplier-sites" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="supplier-site" minOccurs="0" maxOccurs="unbounded" type="SupplierSite"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="taggings" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="tagging" minOccurs="0" maxOccurs="unbounded" type="Tagging"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="tax-code" type="TaxCode" minOccurs="0"/>
                <xs:element name="tax-id" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier DUNS number</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="updated-by" type="UserSimple" minOccurs="0"/>
                <xs:element name="website" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Supplier website</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:maxLength value="255"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="whitelist-dd" type="xs:boolean" minOccurs="0">
                    <xs:annotation>
                        <xs:documentation>Whitelist suppliers for Dynamic Discounting</xs:documentation>
                    </xs:annotation>
                </xs:element>
                <xs:element name="custom-fields" minOccurs="0">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="prepaid_supplier" type="xs:boolean" minOccurs="0"/>
                            <xs:element name="national_provider_identifier" type="xs:string" minOccurs="0"/>
                            <xs:element name="jurisdiction" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="jurisdiction" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="id" type="xs:string"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="type" type="xs:string" fixed="array"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="supplier_type" minOccurs="0">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="1099"/>
                                        <xs:enumeration value="T4A"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="company_code" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="company_code" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="id" type="xs:string"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="type" type="xs:string" fixed="array"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="purchaseorg" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="purchaseorg" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="id" type="xs:string"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="type" type="xs:string" fixed="array"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="purchase_org" minOccurs="0">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="purchase_org" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="id" type="xs:string"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="type" type="xs:string" fixed="array"/>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="influence" type="xs:boolean" minOccurs="0"/>
                            <xs:element name="supplierscustomfield" type="xs:string" minOccurs="0"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
    <xs:complexType name="Commodity">
        <xs:sequence>
            <xs:element name="active" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>A false value will inactivate the account making it no longer available to users. A true value will make it active and available to users.</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="true"/>
                        <xs:enumeration value="false"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="category" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Category</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="goods"/>
                        <xs:enumeration value="services"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="created-by" type="UserSimple" minOccurs="0"/>
            <xs:element name="deductibility" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Deductibility</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="fully_deductible"/>
                        <xs:enumeration value="partially_deductible"/>
                        <xs:enumeration value="not_deductible"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="name" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>name</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:maxLength value="255"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="parent" type="Commodity" minOccurs="0"/>
            <xs:element name="subcategory" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>Subcategory</xs:documentation>
                </xs:annotation>
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="raw_materials"/>
                        <xs:enumeration value="investment_goods"/>
                        <xs:enumeration value="services_exceptions"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="updated-by" type="UserSimple" minOccurs="0"/>
            <xs:element name="custom-fields" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="gl" type="xs:string" minOccurs="0"/>
                        <xs:element name="common_gl_acct" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="id" type="xs:string"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="commoditiescustomfield" type="xs:string" minOccurs="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="UserSimple">
        <xs:sequence>
